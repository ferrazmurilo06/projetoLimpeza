generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id        Int     @id @default(autoincrement())
  urlImagem String
  nome      String
  descricao String?
  preco     Float
  categoria String
  quantidade Int
  status    String
}
